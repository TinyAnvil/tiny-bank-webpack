    "bignumber.js": "^4.1.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.4",
    "moment": "^2.19.1",
    "moment-timezone": "^0.5.13",
    "velocity-animate": "^1.5.0",
    "vue": "^2.4.4",
    "vue-resource": "^1.3.4",
    "vue-touch": "^2.0.0-beta.4"
  }
