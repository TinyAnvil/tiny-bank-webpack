{
  "name": "tinymoney",
  "private": true,
  "scripts": {
    "lint": "./node_modules/.bin/eslint ./js",
    "trash": "rimraf ./dist",
    "prepare": "npm run lint && npm run trash",
    "dev": "webpack-dev-server",
    "prod": "npm run prepare && webpack --env=prod",
    "budget": "npm run prod && surge ./dist https://budget.tiny.money",
    "demo": "npm run prod && surge ./dist https://demo.tiny.money",
    "deploy": "npm run prod && surge ./dist https://budget.tiny.money && surge ./dist https://demo.tiny.money"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "compression-webpack-plugin": "^1.0.1",
    "css-loader": "^0.28.7",
    "css-mqpacker": "^6.0.1",
    "cssnano": "^3.10.0",
    "eslint": "^4.8.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.4",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "import-glob": "^1.5.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "refresh-browser-webpack-plugin": "^1.0.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "accounting": "^0.4.1",
